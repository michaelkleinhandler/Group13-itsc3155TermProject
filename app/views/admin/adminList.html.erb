<table>
  <th>Email</th>
  <th>First Name</th>
  <th>Last Name</th>
  <th>University</th>

  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.email, show_user_path(user) %></td>
      <td><%= user.firstName %></td>
      <td><%= user.lastName %></td>
      <td><%= user.university %></td>
    </tr>
<% end %>
</table>